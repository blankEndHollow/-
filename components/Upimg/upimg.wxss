/* components/Upimg/upimg.wxss */
.up_img_wrap{
  width: 90rpx;
  height: 90rpx;
  margin: 0 0 18rpx 24rpx;
  position: relative;
}
.up_img_wrap .image{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}
.up_img_wrap .icon{
  transform: translate(50%,-50%);
  z-index: 999;
  position: absolute;
  right:0;
  top: 0;
}
